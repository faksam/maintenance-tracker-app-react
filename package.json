{
  "name": "react-webpack4",
  "version": "0.1.1",
  "description": "a simple react and webpack4 setup",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest --env=jsdom --coverage",
    "report-coverage": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "Orjiewuru Kingdom (kingisaac95)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-classes": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.17.0",
    "webpack-dev-server": "^3.1.5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/testSetup.js",
    "collectCoverageFrom": [
      "src/app/**/*.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 30,
        "lines": 50,
        "statements": 50
      }
    },
    "coverageReporters": [
      "cobertura",
      "html",
      "text",
      "text-summary"
    ]
  }
}
